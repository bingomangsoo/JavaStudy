package java0628;

public class ㅋㅊㅌㅍㅋㅌㅊㅍㅋㅌㅊㅍ {
	
	public static void main(String[] args) {
		for (int i =2; i<10; i++) {
			System.out.println(i +"단 : ");
			for (int j =1; j<10; j++) {
				for (int k =i; k<10; k++) {
					System.out.println(i + "*" + j + "=" + (i*j) +" ");
				}
			}
			System.out.println();
		}
		
	}

}
