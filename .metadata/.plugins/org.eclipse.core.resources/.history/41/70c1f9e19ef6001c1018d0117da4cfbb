package java0628;

public class ex09세로로구구단출력 {

	public static void main(String[] args) {
		// TODO Auto-generated method stub

			
			for (int i =1; i<10; i++) {
				System.out.print(i +"단 : ");
				for (int j =2; j<10; j++) {
					System.out.print(j + "*" + i + "=" + (i*j) +"\t");
					
				}
				System.out.println();
			}

	}

}
