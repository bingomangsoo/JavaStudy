package java0628;

public class ex07 {

	public static void main(String[] args) {
		
		
		for (int i =1; i<10; i++) {
			System.out.print(2);
			System.out.print("*");
			System.out.print(i);
			System.out.print("=");
			System.out.println(2*i);
		}
		
		int two =2;
		
		for (int j =1; j<10; j++) {
			System.out.println(two + "*" + j + "=" + (2*j));
		}
	}

}
