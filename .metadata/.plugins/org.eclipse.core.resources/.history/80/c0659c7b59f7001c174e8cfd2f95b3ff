package java0629;

public class ex05약수구하기 {

	public static void main(String[] args) {
		
		for (int i=2; i<31; i++) {
			System.out.print(i+"의 약수 : ");
			System.out.println();
		}
		
//		for (int i=1; i<=7; i++) {
//			if(i%7==0 , i%6==0) { //약수인지 아닌지 판별
//				System.out.println();
//			}
//		}
//		
		
	
		for (int j = 1; j<=10; j++) {
			for (int k=1; k<=10; k++) {
				if (j%k==0) {
				System.out.println(k);
				}
			}
	}
	}
}
