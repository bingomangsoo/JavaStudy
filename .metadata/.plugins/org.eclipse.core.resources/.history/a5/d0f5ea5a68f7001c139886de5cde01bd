package java0629;

public class ex06별찍기과제 {

	public static void main(String[] args) {
		
		for (int j =1; j <=3; j++) {
			for (int i =2; i <=j; i++) {
				System.out.print("@");
			}
		}
		
		
		for (int i =1; i<=2; i++) {
			for (int j =1; j<=i; j++) {
				System.out.print("**");
			}
		
		System.out.println();
		}
		

		
		
		
		
		for (int j =0, k =10; j<=4; j++, k-=2) {
			for (int i=1; i<=j; i++) {
				System.out.print("@");
			}
			for (int i =1; i<=k; i++) {
				System.out.print("*");
			}
			for (int i=1; i<=j; i++) {
				System.out.print("@");
			}
			System.out.println();
		}
		
		
		

	}

}
