package java0628;

public class ㅋㅊㅌㅍㅋㅌㅊㅍㅋㅌㅊㅍ {
	
	public static void main(String[] args) {
		for (int i =2; i<10; i++) {
			System.out.print(i +"단 : ");
		}
		System.out.println();
		for (int i =1; i<10; i++) {
			for (int j =2; j<10; j++) {
					System.out.println(i + "*" + j + "=" + (i*j) +" ");
				}
			}
			System.out.println();

	}

}
