package java0629;

public class ex05약수구하기 {

	public static void main(String[] args) {
		
		for (int i=2; i<31; i++) {
			System.out.println();
		}
		
//		for (int i=1; i<=7; i++) {
//			if(7%i==0) { //약수인지 아닌지 판별
//				System.out.println(i);
//			}
//		}
		
		for (int i=2; i<31; i++) {
			System.out.print(i+"의 약수 : ");

			for (int j=1; j<=i; j++) {
				if (i%j==0) {
					System.out.print(i);
				
				}System.out.println();
			}
		}
		

		
		
		
	
//		for (int j = 1; j<=10; j++) {
//			for (int k=1; k<=10; k++) {
//				if (j%k==0) {
//				System.out.println(k);
//				}
//			}
//	}
	}
}
