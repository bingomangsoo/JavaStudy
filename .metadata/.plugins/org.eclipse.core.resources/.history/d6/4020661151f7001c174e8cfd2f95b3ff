package java0629;

public class ex03좌우반전별찍기 {

	public static void main(String[] args) {
		
		for(int j =1; j<=5; j++) {
			for (int i =2; i <=j; i++) {
				System.out.print(" ");
			}
		
		for(int a=1; a<=5; a++) {
			for (int b =1; b <=a; b++) {
				System.out.print("*");
			}
			System.out.println();
				}

			}

		}


}
